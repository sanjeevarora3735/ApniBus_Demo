class Bus {
  String busNumber;
  Route route;
  String busLocation;
  bool hasGpsInstalled;

  Bus({
    required this.busNumber,
    required this.route,
    required this.busLocation,
    required this.hasGpsInstalled,
  });

  factory Bus.fromJson(Map<String, dynamic> json) {
    return Bus(
      busNumber: json['BusNumber'],
      route: Route.fromJson(json['Route']),
      busLocation: json['BusLocation'],
      hasGpsInstalled: json['HasGpsInstalled'],
    );
  }
}

class Route {
  String to;
  String from;

  Route({
    required this.to,
    required this.from,
  });

  factory Route.fromJson(Map<String, dynamic> json) {
    return Route(
      to: json['to'],
      from: json['from'],
    );
  }
}
